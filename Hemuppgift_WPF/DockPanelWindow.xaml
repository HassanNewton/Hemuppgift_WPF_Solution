<Window x:Class="Hemuppgift_WPF.DockPanelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hemuppgift_WPF"
        mc:Ignorable="d"
        Title="DockPanelWindow" Height="450" Width="800">
    <DockPanel>
        <Button Content="Top" DockPanel.Dock="Top" Margin="5" Height="30" Width="80"/>
        <Button Content="Bottom" DockPanel.Dock="Bottom" Margin="5" Height="30" Width="80"/>
        <Button Content="Left" DockPanel.Dock="Left" Margin="5" Height="30" Width="80"/>
        <Button Content="Right" DockPanel.Dock="Right" Margin="5" Height="30" Width="80"/>
        <Button Content="Center" Margin="5" Height="30" Width="80" Name="CenterButton" Click="CenterButton_Click"/>
    </DockPanel>
</Window>
